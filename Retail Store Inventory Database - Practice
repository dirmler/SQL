CREATE TABLE inventory (id integer primary key, item TEXT, quantity INTEGER, price NUMERIC, category TEXT);

INSERT INTO inventory VALUES (1, "shirts", 30, 15.00, "tops");
INSERT INTO inventory VALUES (2, "jeans", 45, 35.00, "bottoms");
